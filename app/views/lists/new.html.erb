<h1>New List</h1>

<%= form_with model: @list do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
    <% @list.errors.full_messages_for(:name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  

  <div>
    <%= form.submit %>
  </div>
<% end %>